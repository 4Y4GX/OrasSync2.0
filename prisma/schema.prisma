generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model D_tblaccount_recovery_incident {
  incident_id                                                             Int                                           @id @default(autoincrement())
  user_id                                                                 String?                                       @db.VarChar(30)
  incident_type                                                           D_tblaccount_recovery_incident_incident_type?
  status                                                                  D_tblaccount_recovery_incident_status?        @default(OPEN)
  dedupe_key                                                              String                                        @unique(map: "dedupe_key") @db.VarChar(160)
  created_at                                                              DateTime?                                     @default(now()) @db.Timestamp(0)
  resolved_at                                                             DateTime?                                     @db.Timestamp(0)
  resolved_by_user_id                                                     String?                                       @db.VarChar(30)
  D_tbluser_D_tblaccount_recovery_incident_resolved_by_user_idToD_tbluser D_tbluser?                                    @relation("D_tblaccount_recovery_incident_resolved_by_user_idToD_tbluser", fields: [resolved_by_user_id], references: [user_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_incident_resolver")
  D_tbluser_D_tblaccount_recovery_incident_user_idToD_tbluser             D_tbluser?                                    @relation("D_tblaccount_recovery_incident_user_idToD_tbluser", fields: [user_id], references: [user_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_incident_user")

  @@index([resolved_by_user_id], map: "fk_incident_resolver")
  @@index([status], map: "idx_incident_status")
  @@index([user_id], map: "idx_incident_user")
}

model D_tblactivity {
  activity_id          Int                    @id @default(autoincrement())
  activity_code        String?                @db.VarChar(10)
  activity_name        String?                @db.VarChar(50)
  is_billable          Boolean?
  is_active            Boolean?               @default(true)
  created_at           DateTime?              @default(now()) @db.Timestamp(0)
  updated_at           DateTime?              @default(now()) @db.Timestamp(0)
  D_tblfuture_schedule D_tblfuture_schedule[]
  D_tbltime_log        D_tbltime_log[]
}

model D_tblaudit_log {
  audit_id       Int        @id @default(autoincrement())
  changed_by     String?    @db.VarChar(30)
  action_type    String?    @db.VarChar(50)
  table_affected String?    @db.VarChar(50)
  old_value      String?    @db.VarChar(255)
  new_value      String?    @db.VarChar(255)
  created_at     DateTime?  @default(now()) @db.Timestamp(0)
  D_tbluser      D_tbluser? @relation(fields: [changed_by], references: [user_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_audit_user")

  @@index([changed_by], map: "fk_audit_user")
}

model D_tblbreak_log {
  break_id   Int        @id @default(autoincrement())
  user_id    String?    @db.VarChar(30)
  break_type String?    @db.VarChar(50)
  start_time DateTime?  @db.Time(0)
  end_time   DateTime?  @db.Time(0)

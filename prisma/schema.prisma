  D_tbluser  D_tbluser? @relation(fields: [user_id], references: [user_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_break_user")

  @@index([user_id], map: "fk_break_user")
}

model D_tblclock_log {
  clock_id               Int                  @id @default(autoincrement())
  user_id                String?              @db.VarChar(30)
  shift_date             DateTime?            @db.Date
  last_log_in            DateTime?            @default(now()) @db.Timestamp(0)
  clock_in_time          DateTime?            @default(now()) @db.Timestamp(0)
  clock_out_time         DateTime?            @db.Timestamp(0)
  is_sentiment_done      Boolean?             @default(false)
  is_early_leave         Boolean?             @default(false)

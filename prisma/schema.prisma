  D_tblweekly_schedule_D_tblweekly_schedule_tuesday_shift_idToD_tblshift_template   D_tblweekly_schedule[] @relation("D_tblweekly_schedule_tuesday_shift_idToD_tblshift_template")
  D_tblweekly_schedule_D_tblweekly_schedule_wednesday_shift_idToD_tblshift_template D_tblweekly_schedule[] @relation("D_tblweekly_schedule_wednesday_shift_idToD_tblshift_template")
}

model D_tblteam {
